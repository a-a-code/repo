Xft.dpi:                    126
Xft.antialias:              false
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintslight
URxvt.depth:                32
URxvt.fading:               0
URxvt*inheritPixmap:        true
URxvt*transparent:          true
URxvt*shading:              110


! Fonts
URxvt.allow_bold:           true
URxvt*font: xft:Source Code Pro:pixelsize=10:weight=semibold
URxvt*letterSpace: -1

! Scrollbar
URxvt.scrollStyle:          rxvt
URxvt.scrollBar:            false

! Cursor
URxvt.cursorBlink:          true
URxvt.cursorColor:          #657b83! special
URxvt.cursorUnderline:      false
URxvt.pointerBlank:         true

! Theme
*.foreground:   #ffffff
*.background:   #1b1b1b
*.cursorColor:  #ffffff
*.color0:       #3d3d3d
*.color8:       #4d4d4d
*.color1:       #6673bf
*.color9:       #899aff
*.color2:       #3ea290
*.color10:      #52ad91
*.color3:       #b0ead9
*.color11:      #98c9bb
*.color4:       #31658c
*.color12:      #477ab3
*.color5:       #596196
*.color13:      #7882bf
*.color6:       #8292b2
*.color14:      #95a7cc
*.color7:       #c8cacc
*.color15:      #edeff2
